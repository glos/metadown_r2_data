            <gmd:CI_Citation>
               <gmd:title>
                  <gco:CharacterString>Improving Strategies To Restore Aquatic Habitats And Species</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2010</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
