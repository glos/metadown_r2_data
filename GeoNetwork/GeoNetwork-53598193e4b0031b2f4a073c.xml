               <gmd:title>
                  <gco:CharacterString>Toxicity of Organic Chemicals in Mussels</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2013</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
