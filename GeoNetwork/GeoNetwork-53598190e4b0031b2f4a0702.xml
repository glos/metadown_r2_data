               <gmd:title>
                  <gco:CharacterString>Benthic Communities and Sediment Toxicity in the St Lawrence River AOC</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
