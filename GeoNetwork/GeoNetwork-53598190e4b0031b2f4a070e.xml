               <gmd:title>
                  <gco:CharacterString>Lakewide Management Plan Capacity Support by U.S. Geological Survey - LAKE ERIE</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
